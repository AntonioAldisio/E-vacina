<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.Evacina">
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE"/>


    <application
        android:allowBackup="true"
        android:icon="@mipmap/logo"
        android:label="E-Vacina"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.Evacina4"
        android:requestLegacyExternalStorage="true">

        <activity android:name="com.example.Evacina.ui.acitivity.ActivityEnfermeiro"></activity>
        <activity android:name="com.example.Evacina.ui.acitivity.ActivityInformacoesPessoiasEdicao" />
        <activity android:name="com.example.Evacina.ui.acitivity.ActivityCadastroVacina" />
        <activity android:name="com.example.Evacina.ui.acitivity.ActivityInformacoesPessoais" />
        <activity android:name="com.example.Evacina.ui.acitivity.ActivityHomePage" />
        <activity android:name="com.example.Evacina.ui.acitivity.ActivityCadastro" />
        <activity android:name="com.example.Evacina.ui.acitivity.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>