<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/button_sair"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".ui.acitivity.ActivityHomePage">


    <TextView
        android:id="@+id/textAplicador"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="24dp"
        android:layout_marginLeft="24dp"
        android:layout_marginTop="8dp"
        android:text="Aplicador"
        android:textColor="#F2994A"
        android:textSize="18sp"
        app:layout_constraintStart_toEndOf="@+id/textVacina"
        app:layout_constraintTop_toBottomOf="@+id/Activity_homepage_textview_nome_cliente" />

    <TextView
        android:id="@+id/textLote"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="24dp"
        android:layout_marginLeft="24dp"
        android:layout_marginTop="8dp"
        android:text="Lote"
        android:textColor="#F2994A"
        android:textSize="18sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toEndOf="@+id/textData"
        app:layout_constraintTop_toBottomOf="@+id/Activity_homepage_textview_nome_cliente" />

    <Button
        android:id="@+id/Activity_homepage_button_sair"
        android:layout_width="100dp"
        android:layout_height="50dp"
        android:layout_marginEnd="16dp"
        android:layout_marginRight="16dp"
        android:layout_marginBottom="16dp"
        android:text="Sair"
        app:backgroundTint="#F2994A"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent" />

    <TextView
        android:id="@+id/Activity_homepage_textview_nome_cliente"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="4dp"
        android:layout_marginEnd="24dp"
        android:layout_marginRight="24dp"
        android:text="Usuarios"
        android:textColor="#F2994A"
        android:textSize="18sp"
        app:layout_constraintEnd_toEndOf="@+id/textView2"
        app:layout_constraintTop_toBottomOf="@+id/textView2" />

    <TextView
        android:id="@+id/textView2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="32dp"
        android:text="Bem-vindos"
        android:textColor="#F2994A"
        android:textSize="24sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/textVacina"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="8dp"
        android:text="Vacina"
        android:textColor="#F2994A"
        android:textSize="18sp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/Activity_homepage_textview_nome_cliente" />

    <TextView
        android:id="@+id/textData"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="24dp"
        android:layout_marginLeft="24dp"
        android:layout_marginTop="8dp"
        android:text="Data"
        android:textColor="#F2994A"
        android:textSize="18sp"
        app:layout_constraintStart_toEndOf="@+id/textAplicador"
        app:layout_constraintTop_toBottomOf="@+id/Activity_homepage_textview_nome_cliente" />

    <ListView
        android:id="@+id/Activity_homepage_listview_aplicador"
        android:layout_width="0dp"
        android:layout_height="0dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toStartOf="@+id/Activity_homepage_listview_data"
        app:layout_constraintStart_toEndOf="@+id/Activity_homepage_listview_vacina"
        app:layout_constraintTop_toBottomOf="@+id/textAplicador" />

    <ListView
        android:id="@+id/Activity_homepage_listview_lote"
        android:layout_width="0dp"
        android:layout_height="0dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toEndOf="@+id/Activity_homepage_listview_data"
        app:layout_constraintTop_toBottomOf="@+id/textLote" />

    <ListView
        android:id="@+id/Activity_homepage_listview_data"
        android:layout_width="0dp"
        android:layout_height="0dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toStartOf="@+id/Activity_homepage_listview_lote"
        app:layout_constraintStart_toEndOf="@+id/Activity_homepage_listview_aplicador"
        app:layout_constraintTop_toBottomOf="@+id/textData" />

    <ListView
        android:id="@+id/Activity_homepage_listview_vacina"
        android:layout_width="79dp"
        android:layout_height="0dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toStartOf="@+id/Activity_homepage_listview_aplicador"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textVacina" />

    <Button
        android:id="@+id/Activity_homepage_button_PDF"
        android:layout_width="100dp"
        android:layout_height="50dp"
        android:layout_marginStart="16dp"
        android:layout_marginLeft="16dp"
        android:layout_marginBottom="16dp"
        android:text="PDF"
        app:backgroundTint="#F2994A"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintStart_toStartOf="parent" />


</androidx.constraintlayout.widget.ConstraintLayout>